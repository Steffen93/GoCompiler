help := 2. * 18. - 6. 			//33.60
a := 2.*2.*2.*2.*2. * help		//1075.20
help := help * 2.			//67.20
func test (b float) float{
  dk := 2. * b
  //dk := dk + 1.0
  return dk
}
ko := 12. * help / a - 4. test		//-3.25
q := "Das ist " + "ein test"		//Das ist ein test
bla := ko * ko + test(8.)				//10.56
i := 2 + 2
a := 'g' + '5'
b := 'c'
(q + "test")
